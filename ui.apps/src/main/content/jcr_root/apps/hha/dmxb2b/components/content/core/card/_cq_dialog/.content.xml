<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Compare Plan Card"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <basic
                jcr:primaryType="nt:unstructured"
                jcr:title="Compare Plan Card"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Heading of the card"
                                fieldLabel="Heading"
                                name="./heading"/>
                            <subheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Sub heading of the card"
                                fieldLabel="Sub-Heading"
                                name="./subheading"/>
                            <multinavlinks
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Links">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./links">
                                    <items jcr:primaryType="nt:unstructured">
                                        <label
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Label"
                                            name="label"/>
                                    </items>
                                </field>
                            </multinavlinks>
                            <buttontext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Text Of Button"
                                fieldLabel="Button Text"
                                name="./buttontext"/>
                            <button-types
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Button Types"
                                fieldLabel="Button Types"
                                name="./buttontypes">
                                <items jcr:primaryType="nt:unstructured">
                                    <none
                                        jcr:primaryType="nt:unstructured"
                                        text="None"
                                        value=""/>
                                    <button-regular
                                        jcr:primaryType="nt:unstructured"
                                        text="button regular"
                                        value="button-core-sm"/>
                                    <button-large
                                        jcr:primaryType="nt:unstructured"
                                        text="button large"
                                        value="button-core-lg"/>
                                    <btn-signup-small
                                        jcr:primaryType="nt:unstructured"
                                        text="button signup small"
                                        value="button-signup-sm"/>
                                    <btn-signup-large
                                        jcr:primaryType="nt:unstructured"
                                        text="btn signup large"
                                        value="button-signup-lg"/>
                                </items>
                            </button-types>
                        </items>
                    </column>
                </items>
            </basic>
        </items>
    </content>
</jcr:root>
