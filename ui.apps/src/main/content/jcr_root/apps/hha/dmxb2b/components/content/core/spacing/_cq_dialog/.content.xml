<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Space Content"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="en/cq/current/wcm/default_components.html#Text">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <fieldset
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Space&#xa;"
                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <Desktop-top-bottom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Desktop Top and Bottom Padding"
                                        fieldLabel="Desktop Top and Bottom padding"
                                        name="./desktopspacingvertical">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <ten
                                                jcr:primaryType="nt:unstructured"
                                                text="10px padding"
                                                value="ten-padding"/>
                                            <twenty
                                                jcr:primaryType="nt:unstructured"
                                                text="20px padding"
                                                value="twenty-padding"/>
                                            <thirty
                                                jcr:primaryType="nt:unstructured"
                                                text="30px padding"
                                                value="thirty-padding"/>
                                            <forty
                                                jcr:primaryType="nt:unstructured"
                                                text="40px padding"
                                                value="forty-padding"/>
                                            <fifty
                                                jcr:primaryType="nt:unstructured"
                                                text="50px padding"
                                                value="fifty-padding"/>
                                            <sixty
                                                jcr:primaryType="nt:unstructured"
                                                text="60px padding"
                                                value="sixty-padding"/>
                                            <eighty
                                                jcr:primaryType="nt:unstructured"
                                                text="80px padding"
                                                value="eighty-padding"/>
                                            <hundred
                                                jcr:primaryType="nt:unstructured"
                                                text="100px padding"
                                                value="hundred-padding"/>
                                        </items>
                                    </Desktop-top-bottom>
                                    <tablet-top-bottom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Tablet Top and Bottom Padding"
                                        fieldLabel="Tablet Top and Bottom padding"
                                        name="./tabletspacingvertical">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <ten
                                                jcr:primaryType="nt:unstructured"
                                                text="10px padding"
                                                value="tab-ten-padding"/>
                                            <twenty
                                                jcr:primaryType="nt:unstructured"
                                                text="20px padding"
                                                value="tab-twenty-padding"/>
                                            <thirty
                                                jcr:primaryType="nt:unstructured"
                                                text="30px padding"
                                                value="tab-thirty-padding"/>
                                            <forty
                                                jcr:primaryType="nt:unstructured"
                                                text="40px padding"
                                                value="tab-forty-padding"/>
                                            <fifty
                                                jcr:primaryType="nt:unstructured"
                                                text="50px padding"
                                                value="tab-fifty-padding"/>
                                            <sixty
                                                jcr:primaryType="nt:unstructured"
                                                text="60px padding"
                                                value="tab-sixty-padding"/>
                                        </items>
                                    </tablet-top-bottom>
                                    <Mobile-top-bottom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Mobile Top and Bottom Padding"
                                        fieldLabel="MobileTop and Bottom padding"
                                        name="./mobilespacingvertical">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <ten
                                                jcr:primaryType="nt:unstructured"
                                                text="10px padding"
                                                value="mob-ten-padding"/>
                                            <twenty
                                                jcr:primaryType="nt:unstructured"
                                                text="20px padding"
                                                value="mob-twenty-padding"/>
                                            <thirty
                                                jcr:primaryType="nt:unstructured"
                                                text="30px padding"
                                                value="mob-thirty-padding"/>
                                            <forty
                                                jcr:primaryType="nt:unstructured"
                                                text="40px padding"
                                                value="mob-forty-padding"/>
                                            <fifty
                                                jcr:primaryType="nt:unstructured"
                                                text="50px padding"
                                                value="mob-fifty-padding"/>
                                        </items>
                                    </Mobile-top-bottom>
                                </items>
                            </column>
                        </items>
                    </fieldset>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
